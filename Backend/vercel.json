{
    "version": 2,
    "builds": [
      {
        "src": "Frontend/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/Frontend/$1"
      }
    ],
    "env": {
      "VITE_PUBLIC_BASE_URL": "http://localhost:8000/api",  
      "NODE_ENV": "production"
    }
  }
  